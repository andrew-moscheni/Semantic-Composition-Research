[{"word": "disappointed", "phonetic": "/\u02ccd\u026as\u0259\u02c8p\u0254\u026ant\u026ad/", "phonetics": [{"text": "/\u02ccd\u026as\u0259\u02c8p\u0254\u026ant\u026ad/", "audio": "https://api.dictionaryapi.dev/media/pronunciations/en/disappointed-us.mp3", "sourceUrl": "https://commons.wikimedia.org/w/index.php?curid=2100373", "license": {"name": "BY-SA 3.0", "url": "https://creativecommons.org/licenses/by-sa/3.0"}}], "meanings": [{"partOfSpeech": "verb", "definitions": [{"definition": "To sadden or displease (someone) by underperforming, or by not delivering something promised or hoped for.", "synonyms": [], "antonyms": [], "example": "His lack of respect disappointed her."}, {"definition": "To deprive (someone of something expected or hoped for).", "synonyms": [], "antonyms": []}, {"definition": "To fail to meet (an expectation); to fail to fulfil (a hope).", "synonyms": [], "antonyms": []}, {"definition": "To show (an opinion, belief, etc.) to be mistaken.", "synonyms": [], "antonyms": []}, {"definition": "To prevent (something planned or attempted).", "synonyms": ["frustrate", "thwart"], "antonyms": []}], "synonyms": ["frustrate", "thwart"], "antonyms": ["satisfy"]}, {"partOfSpeech": "adjective", "definitions": [{"definition": "Defeated of expectations or hope; experiencing disappointment; let down.", "synonyms": [], "antonyms": [], "example": "After all his anticipation, the trip left him deeply disappointed."}, {"definition": "Expressing or indicating disappointment.", "synonyms": [], "antonyms": [], "example": "a disappointed tone / face / silence"}], "synonyms": ["discomfited", "foiled", "frustrated", "thwarted"], "antonyms": []}], "license": {"name": "CC BY-SA 3.0", "url": "https://creativecommons.org/licenses/by-sa/3.0"}, "sourceUrls": ["https://en.wiktionary.org/wiki/disappoint", "https://en.wiktionary.org/wiki/disappointed"]}]